{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "declaration": true,
    "downlevelIteration": true,
    "esModuleInterop": true,
    "inlineSourceMap": true,
    "inlineSources": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "outDir": "../dist",
    "pretty": true,
    "resolveJsonModule": true,
    "strict": true,
    "target": "es2017",
    "lib": [
      "es5",
      "es6",
      "es7",
      "esnext"
    ]
  }
}